            if( $(this).val().length > 0 ){
                searchList.hide();
            }
            /*输入的时val()大于0推荐搜索隐藏*/
            $(this).keyup(function() {
                if( $(this).val().length > 0 ){
                    searchList.hide();
                    $("#hotwords").show();
                    $(".search-default").hide();
                }else{
                    searchList.show();
                    $("#hotwords").hide();
